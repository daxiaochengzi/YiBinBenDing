
@{
    ViewBag.Title = "门诊结算";
    Layout = "~/Views/Shared/_Base.cshtml";
}

<link href="~/Content/js/layui/css/modules/layuiAutocomplete.css" rel="stylesheet" />
<script src="~/script/BenDingComm.js"></script>
<style>
    .layui-card-header.layuibd-card-header-auto {
        padding-top: 15px;
        padding-bottom: 15px;
        height: auto;
    }

    .layuibd-card-header-auto i.layuibd-button-btn {
        position: relative;
        right: 0;
        top: 0;
        vertical-align: middle
    }
</style>
<div class="layui-fluid">
    <div class="layui-card">
        <form class="layui-form" id="LAY-form" lay-filter="LAY-form-render">
            @{Html.RenderPartial("_CSharpActiveX");}
            <div class="layui-card-header layuibd-card-header-auto">
                <fieldset class="layui-elem-field layui-field-title" style="margin-top: 10px;">
                    <legend>病人信息</legend>
                </fieldset>
                <!--病人信息-->
                <div class="layui-form-item">
                    <label class="layui-form-label">结算方式</label>
                    <div class="layui-input-block">
                        <input type="radio" id="SettlementThree" name="Settlement" value="2" checked="checked" title="门诊刷卡结算">
                        <input type="radio" id="SettlementFour" name="Settlement" value="4" checked="checked" title="门诊居民报账">
                        <input type="radio" id="SettlementTwo" name="Settlement" value="1" title="职工计划生育结算">
                        <input type="radio" id="SettlementOne" name="Settlement" value="3" title="医保电子凭证结算">
                    </div>

                </div>
                <div class="layui-inline">
                    <label class="layui-form-label">主诊断编码</label>
                    <div class="layui-input-inline">
                        <input type="text" name="DiagnosisCode" autocomplete="off" disabled class="layui-input layui-disabled">
                    </div>
                </div>
                <div class="layui-inline">
                    <label class="layui-form-label">主诊断名称</label>
                    <div class="layui-input-inline">
                        <input type="text" name="DiagnosisName" autocomplete="off" disabled class="layui-input layui-disabled">
                    </div>
                </div>
                <div class="layui-inline">
                    <label class="layui-form-label">医保诊断编码</label>
                    <div class="layui-input-inline">
                        <input type="text" name="DiagnosisMedicalInsuranceCode" autocomplete="off" disabled class="layui-input layui-disabled">
                    </div>
                </div>
                <div class="layui-inline">
                    <label class="layui-form-label">医保诊断名称</label>
                    <div class="layui-input-inline">
                        <input type="text" name="DiagnosisMedicalInsuranceName" autocomplete="off" disabled class="layui-input layui-disabled">
                    </div>
                </div>
                <div class="layui-inline">
                    <label class="layui-form-label">门诊号</label>
                    <div class="layui-input-inline">
                        <input type="text" name="OutpatientNumber" autocomplete="off" disabled class="layui-input layui-disabled">
                    </div>
                </div>
                <div class="layui-inline">
                    <label class="layui-form-label">身份证号</label>
                    <div class="layui-input-inline">
                        <input type="text" name="IdCardNo" @*lay-verify="required|number"*@ autocomplete="off" disabled class="layui-input layui-disabled">
                    </div>
                </div>
                <div class="layui-inline">
                    <label class="layui-form-label">患者姓名</label>
                    <div class="layui-input-inline">
                        <input type="text" name="PatientName" autocomplete="off" disabled class="layui-input layui-disabled">
                    </div>
                </div>
                <div class="layui-inline">
                    <label class="layui-form-label">就诊日期</label>
                    <div class="layui-input-inline">
                        <input type="text" name="VisitDate" autocomplete="off" disabled class="layui-input layui-disabled">
                    </div>
                </div>
                <div class="layui-inline">
                    <label class="layui-form-label">住院科室</label>
                    <div class="layui-input-inline">
                        <input type="text" name="DepartmentName" autocomplete="off" disabled class="layui-input layui-disabled">
                    </div>
                </div>
                <div class="layui-inline">
                    <label class="layui-form-label">费用合计</label>
                    <div class="layui-input-inline">
                        <input type="text" name="MedicalTreatmentTotalCost" autocomplete="off" disabled class="layui-input layui-disabled">
                    </div>
                </div>
                <div class="layui-inline">
                    <label class="layui-form-label">现金支付</label>
                    <div class="layui-input-inline">
                        <input type="text" id="CashPayment" name="CashPayment" autocomplete="off" disabled class="layui-input layui-disabled">
                    </div>
                </div>
                <div class="layui-inline" id="InsuranceBalanceDiv">
                    <label class="layui-form-label">账户余额</label>
                    <div class="layui-input-inline">
                        <input type="text" id="InsuranceBalance" name="InsuranceBalance" autocomplete="off" disabled class="layui-input layui-disabled">
                    </div>
                </div>
                <div class="layui-inline" id="AccountPaymentDiv">
                    <label class="layui-form-label">下账金额</label>
                    <div class="layui-input-inline">
                        <input type="text" id="AccountPayment" name="AccountPayment" autocomplete="off" class="layui-input">
                    </div>
                </div>
                <div class="layui-inline">
                    <label class="layui-form-label">诊断医生</label>
                    <div class="layui-input-inline">
                        <input type="text" name="DiagnosticDoctor" autocomplete="off" disabled class="layui-input layui-disabled">
                    </div>
                </div>
                <div class="layui-inline">
                    <label class="layui-form-label">经办人</label>
                    <div class="layui-input-inline">
                        <input type="text" name="Operator" autocomplete="off" disabled class="layui-input layui-disabled">
                    </div>
                </div>
                <div class="layui-inline">
                    <label class="layui-form-label">发票号</label>
                    <div class="layui-input-inline">
                        <input type="text" name="InvoiceNo" autocomplete="off" disabled class="layui-input layui-disabled">
                    </div>
                </div>
       
                <div class="layui-form-item" id="passwordDiv">
                    <label class="layui-form-label">医保卡密码</label>
                    <div class="layui-input-inline">
                        <input type="password" Id="CardPwd" name="password" lay-verify="pass" placeholder="请输入密码" autocomplete="off" class="layui-input">
                    </div>
                </div>
            </div>
            <div class="layui-form-item" id="queryDataDiv" style="text-align: center; margin-top: 20px;">
                <button type="button" Id="queryData" class="layui-btn layui-btn-radius">查询</button>

            </div>
            <div class="layui-form-item" id="DoorDiagnosisPreSettlementDiv" style="text-align: center; margin-top: 20px;">
                <button type="button" Id="SettlementCard" class="layui-btn layui-btn-disabled layui-btn-radius">门诊职工刷卡结算</button>
                <button type="button" Id="ResidentSettlementCard" class="layui-btn layui-btn-disabled layui-btn-radius">门诊居民结算</button>
                <button type="button" Id="DoorDiagnosisPreSettlement" class="layui-btn layui-btn-disabled layui-btn-radius">职工计划生育预结算</button>
                <button type="button" Id="OutpatientNationEcTrans" class="layui-btn layui-btn-disabled layui-btn-radius">医保电子凭证结算</button>
            </div>
            <div class="layui-form-item" style="text-align: center; margin-top: 20px;" id="DoorDiagnosisSettlementDiv">
                <button type="button" Id="DoorDiagnosisSettlement" class="layui-btn layui-btn-disabled layui-btn-radius">职工计划生结算</button>

            </div>
            <div class="layui-form-item" style="text-align: center; margin-top: 20px;" id="ResidentSettlementCardDiv">
                <button type="button" Id="DoorDiagnosisResidentCardSettlement" class="layui-btn layui-btn-disabled layui-btn-radius">门诊居民划卡</button>
                <button type="button" Id="CompleteSettlement" class="layui-btn layui-btn-disabled layui-btn-radius">完成结算</button>
                
            </div>
            <div class="layui-card-body" lay-size="sm">
                <fieldset id="SettleData" class="layui-elem-field layui-field-title" style="margin-top: 10px;">
                    <legend>医保结算信息</legend>
                </fieldset>
            </div>

        </form>
    </div>
</div>
<script>
    layui.config({
        base: '/Content/js/layui/plugin/'
    }).extend({
        autocomplete: 'layAutoComplete'
        });
    var iniJs;
    var iniMsg;
    layui.use(['form', 'laypage', 'layer', 'table', 'element', 'laydate', 'autocomplete'], function () {
        var $ = layui.$,
            element = layui.element,
            form = layui.form,
            layer = layui.layer,
            table = layui.table;
        //初始化公共页面
        iniJs = $;
        iniMsg = layer;
        $("#InsuranceBalanceDiv").hide();
        $("#AccountPaymentDiv").hide();
        $("#DoorDiagnosisPreSettlementDiv").hide();
        $("#DoorDiagnosisSettlementDiv").hide();
        $("#queryDataDiv").hide();
        $("#passwordDiv").hide();
        
        baseInfo.PageFunction.FunctionName = "Outpatient";
        var passWordKey = "1";
       // 插件检查
        var verification = DetectActiveX();
        if (verification === false) {
            return false;
        }
        //是否使用加密键盘
        function isPassKey() {
            var activeX = document.getElementById("CSharpActiveX");
            var activeData = activeX.CheckPwd();
            if (activeData === "0") {
                passWordKey = "0";
                $("#passwordDiv").show();
                $("#CardPwd").focus();
            }
        }
        //form数据回显:数据回显到Form表单主体部分
        var jsonToForm = function (jsonData, formFilter) {
            var diagnosisList = jsonData.DiagnosisList;
            var diagnosisName = null;
            var diagnosisCode = null;
            var diagnosisMedicalInsuranceCode = null;
            var diagnosisMedicalInsuranceName = null;
            if (diagnosisList !== null) {

                for (var i = 0; i < diagnosisList.length; i++) {
                    if (diagnosisList[i].是否主诊断 === "是") {
                        diagnosisName = diagnosisList[i].诊断名称;
                        diagnosisCode = diagnosisList[i].诊断编码;
                        diagnosisMedicalInsuranceCode = diagnosisList[i].诊断医保编码;
                        diagnosisMedicalInsuranceName = diagnosisList[i].诊断医保名称;
                    }
                }

            }
            //给表单赋值
            form.val(formFilter,
                {
                    "OutpatientNumber": jsonData.OutpatientNumber,
                    "IdCardNo": jsonData.IdCardNo,
                    "PatientName": jsonData.PatientName,
                    "VisitDate": jsonData.VisitDate,
                    "DepartmentName": jsonData.DepartmentName,
                    "MedicalTreatmentTotalCost": jsonData.MedicalTreatmentTotalCost,
                    "Operator": jsonData.Operator,
                    "DiagnosticDoctor": jsonData.DiagnosticDoctor,
                    "InvoiceNo": jsonData.InvoiceNo,
                    "DiagnosisName": diagnosisName,
                    "DiagnosisCode": diagnosisCode,
                    "DiagnosisMedicalInsuranceCode": diagnosisMedicalInsuranceCode,
                    "DiagnosisMedicalInsuranceName": diagnosisMedicalInsuranceName
                });

        }
       
        var params = {
            "TransKey": $("#transkey").val() /*医保交易码*/,
            "BusinessId": $("#bid").val() /*当前住院记录的业务ID*/,
            "UserId": $("#empid").val() /*授权操作人的ID*/
        }
        params["timestamp"] = (new Date()).valueOf();
        //获取医院基本信息回调方法
        window.getHospitalInfoBack = function () { };
     
        getHospitalInfo(getHospitalInfoBack);
        //结算方式控制
        function radioChecked() {
            $("#SettlementOne,#SettlementTwo,#SettlementThree,#SettlementFour").attr("disabled","");
        }
        //初始化病人基本信息
        function iniInpatientInfo() {
            // 居民
            if (baseInfo.Inpatient.InsuranceType === "342") {
                $("#InsuranceBalance").val(baseInfo.Inpatient.ResidentInsuranceBalance);
                //职工
            } else if (baseInfo.Inpatient.InsuranceType === "310") {
                $("#InsuranceBalance").val(baseInfo.Inpatient.WorkersInsuranceBalance);
            }
            var settlementVal = $('input[name="Settlement"]:checked').val();
            $("#DoorDiagnosisPreSettlementDiv").show();
            //计划生育结算
            if (settlementVal === "1" && baseInfo.Inpatient.InsuranceType === "310") {
                           
                buttonStatus("DoorDiagnosisPreSettlement", true);
            }
            if (settlementVal === "3" )//电子凭证结算
            {
                buttonStatus("OutpatientNationEcTrans", true);
            }
            //刷卡结算
            if (settlementVal === "2" && baseInfo.Inpatient.InsuranceType === "310" )
            {
                buttonStatus("SettlementCard", true);
                isPassKey();
            }
            //居民刷卡结算
            if (settlementVal === "2" || settlementVal === "4") {
                if (baseInfo.Inpatient.InsuranceType === "342") {
                    buttonStatus("SettlementCard", true);
                    isPassKey();
                }
              
            }
           
            //if (settlementVal === "4" )//门诊结算
            //{////显示结算按钮
            //    //$("#DoorDiagnosisSettlementDiv").show();
            //    ////隐藏预结算按钮
            //    //$("#DoorDiagnosisPreSettlementDiv").hide();
            //    ////居民显示结算按钮
            //    //buttonStatus("DoorDiagnosisSettlement", true);
            //}
            $("#InsuranceBalanceDiv").show();
            params["AfferentSign"] =  baseInfo.HospitalInfo.AfferentSign;
            params["IdentityMark"] = baseInfo.HospitalInfo.IdentityMark;
            params["InsuranceType"] = baseInfo.Inpatient.InsuranceType;
        }
        //获取医保病人信息
        window.getInpatientInfoBack = function () {
            iniInpatientInfo();
        }
        //获取门诊结算参数
        function getOutpatientDepartmentCostInputParam() {

           
            $.ajax({
                type: 'post',
                url: hostNew + '/GetOutpatientDepartmentCostInputParam',
                data: params,
                dataType: "json",
                async: false,
                success: function (data) {
                 
                    if (data.Success === false) {
                        var errData = data.Message;
                        layer.alert(errData, { skin: 'layui-layer-molv', icon: 5, title: '错误提示' });
                        //样式类名:墨绿深蓝风

                    } else {
                        var activeX = document.getElementById("CSharpActiveX");
                        //门诊结算
                        var activeData = activeX.OutpatientMethods(data.Data,JSON.stringify(baseInfo.HospitalInfo),"OutpatientDepartmentCostInput");
                        var activeJsonData = JSON.parse(activeData);
                        if (activeJsonData.Success === false) {
                            msgError(activeJsonData.Message);
                        } else {
                            outpatientDepartmentCostInput(activeJsonData.Data, activeJsonData.OtherInfo);
                        }
                       
                    }
                }

            });
        }
        //普通门诊结算
        function outpatientDepartmentCostInput(costInputData, accountBalance) {
            var costInputParam = $.extend({}, params);;
            costInputParam["SettlementJson"] = costInputData;
            costInputParam["AccountBalance"] = accountBalance;
            $.ajax({
                type: 'post',
                url: hostNew + '/OutpatientDepartmentCostInput',
                data: costInputParam,
                dataType: "json",
                async: false,
                success: function (data) {
                 
                    if (data.Success === false) {
                        var errData = data.Message;
                        msgError(errData);

                    } else {
                        //$('#ReimbursementExpenses').val(data.Data.ReimbursementExpensesAmount);
                        $('#CashPayment').val(data.Data.SelfPayFeeAmount);
                        msgSuccess('门诊结算成功');
                        buttonStatus("DoorDiagnosisSettlement", false);
                        //添加结算信息
                        if (data.Data.PayMsg !== null) {
                            settlementData(data.Data.PayMsg);
                        }
                    }
                }

            });
        }
        //获取门诊计划生育预结算参数
        function getOutpatientPlanBirthPreSettlementParam() {
            //隐藏预结算按钮
            $("#DoorDiagnosisPreSettlementDiv").hide();
          
            $.ajax({
                type: 'POST',
                url: hostNew + '/GetOutpatientPlanBirthPreSettlementParam',
                data: params,
                dataType: "json",
                async: false,
                success: function (data) {
                    if (data.Success === false) {
                        var errData = data.Message;
                        layer.alert(errData, { skin: 'layui-layer-molv', icon: 5, title: '错误提示' });
                    } else {
                        var activeX = document.getElementById("CSharpActiveX");
                       
                        //门诊计划生育预结算
                        var activeData = activeX.OutpatientMethods(data.Data, JSON.stringify(baseInfo.HospitalInfo),"OutpatientPlanBirthPreSettlement");
                        var activeJsonData = JSON.parse(activeData);
                        if (activeJsonData.Success === false) {
                            msgError(activeJsonData.Message);
                        } else {
                            outpatientPlanBirthPreSettlement(activeJsonData.Data);
                        }
                    }
                }

            });

        }
        //门诊计划生育预结算
        function outpatientPlanBirthPreSettlement(settlementJsonData) {
           
            var preParam = $.extend({}, params);;
            preParam["SettlementJson"] = settlementJsonData;
            $.ajax({
                type: 'post',
                url: hostNew + '/OutpatientPlanBirthPreSettlement',
                data: preParam,
                dataType: "json",
                async: false,
                success: function (data) {
                    
                
                    if (data.Success === false) {
                        var errData = data.Message;
                        msgError(errData);
                      

                    } else {
                        $('#CashPayment').val(data.Data.SelfPayFeeAmount);
                        layer.alert('门诊预结算成功', { icon: 6, shade: 0.1, skin: 'layui-layer-molv' });
                        //隐藏预结算按钮
                        $("#DoorDiagnosisPreSettlementDiv").hide();
                        $("#DoorDiagnosisSettlementDiv").show();
                        $("#AccountPaymentDiv").show();
                      
                        //显示结算按钮
                        buttonStatus("DoorDiagnosisSettlement", true);
                        
                        //添加预结算信息
                        if (data.Data.PayMsg !== null) {
                       
                            settlementData(data.Data.PayMsg);
                        }
                    }
                }

            });
        }
        //获取门诊计划生育结算参数
        function getOutpatientPlanBirthSettlementParam() {
           
            $.ajax({
                type: 'POST',
                url: hostNew + '/GetOutpatientPlanBirthSettlementParam',
                data: params,
                dataType: "json",
                async: false,
                success: function (data) {
                    if (data.Success === false) {
                        var errData = data.Message;
                        layer.alert(errData, { skin: 'layui-layer-molv', icon: 5, title: '错误提示' });
                    } else {
                        var activeX = document.getElementById("CSharpActiveX");
                        //门诊计划生育结算
                      
                        var activeData = activeX.OutpatientMethods(data.Data, JSON.stringify(baseInfo.HospitalInfo),"OutpatientPlanBirthSettlement");
                        var activeJsonData = JSON.parse(activeData);
                        if (activeJsonData.Success === false) {
                            msgError(activeJsonData.Message);
                        } else {
                            outpatientPlanBirthSettlement(activeJsonData.Data, activeJsonData.OtherInfo);
                        }
                    }
                }

            });

        }
        //门诊计划生育结算
        function outpatientPlanBirthSettlement(settlementDataInfo, accountBalance) {
            var planBirthParams = $.extend({}, params);
            planBirthParams["SettlementJson"] = settlementDataInfo;
            planBirthParams["AccountBalance"] = accountBalance;
            $.ajax({
                type: 'post',
                url: hostNew + '/OutpatientDepartmentCostInput',
                data: planBirthParams,
                dataType: "json",
                async: false,
                success: function (data) {
                    
                    if (data.Success === false) {  
                        var errData = data.Message;
                        msgError(errData);
                    } else {
                        $('#CashPayment').val(data.Data.SelfPayFeeAmount);
                        layer.alert('门诊结算成功', { icon: 6, shade: 0.1, skin: 'layui-layer-molv' });
                       
                        $("#DoorDiagnosisSettlementDiv").show();
                        $("#AccountPaymentDiv").show();
                        //取消显示结算按钮
                        buttonStatus("DoorDiagnosisSettlement", false);
                        //添加预结算信息
                        if (data.Data.PayMsg !== null) {
                            settlementData(data.Data.PayMsg);
                        }
                    }
                }

            });
        }
        //获取电子社保支付参数
        function outpatientNationEcTransParam() {
            var url = "/OutpatientNationEcTransParam";
            if (baseInfo.Inpatient.InsuranceType !== "310") url = "/GetOutpatientNationEcTransResidentParam";
            $.ajax({
                type: 'POST',
                url: hostNew + url,
                data: params,
                dataType: "json",
                async: false,
                success: function (data) {
                    if (data.Success === false) {
                        var errData = data.Message;
                        layer.alert(errData, { skin: 'layui-layer-molv', icon: 5, title: '错误提示' });
                    } else {
                        var activeX = document.getElementById("CSharpActiveX");
                        var activeData;
                        var activeJsonData; 
                        //电子医保支付
                        if (baseInfo.Inpatient.InsuranceType !== "310") {
                            activeData = activeX.OutpatientMethods(data.Data,
                                JSON.stringify(baseInfo.HospitalInfo),
                                "NationEcTrans");
                            activeJsonData = JSON.parse(activeData);
                        }
                        else {
                            activeData = activeX.OutpatientMethods(data.Data,
                                JSON.stringify(baseInfo.HospitalInfo),
                                "NationEcTransResident");
                              activeJsonData = JSON.parse(activeData);
                        }
                         
                        if (activeJsonData.Success === false) {
                            msgError(activeJsonData.Message);
                        } else {
                            outpatientNationEcTrans(activeJsonData.Data);
                        }

                    }
                }

            });
        }
        //电子社保支付
        function outpatientNationEcTrans(settlementDataInfo) {
            var ecTransParam = $.extend({}, params);
            var url = "/outpatientNationEcTrans";
            if (baseInfo.Inpatient.InsuranceType !== "310") url = "/OutpatientNationEcTransResident";
            ecTransParam["SettlementJson"] = settlementDataInfo;
            $.ajax({
                type: 'POST',
                url: hostNew + url,
                data: ecTransParam,
                dataType: "json",
                async: false,
                success: function (data) {
                    if (data.Success === false) {
                        var errData = data.Message;
                        layer.alert(errData, { skin: 'layui-layer-molv', icon: 5, title: '错误提示' });
                    } else {
                        $('#CashPayment').val(data.Data.SelfPayFeeAmount);
                      
                        layer.alert('电子凭证支付成功', { icon: 6, shade: 0.1, skin: 'layui-layer-molv' });
                        //添加预结算信息
                        if (data.Data.PayMsg !== null) {
                            settlementData(data.Data.PayMsg);
                        }
                    }
                }

            });
        }

        //获取门诊划卡参数
        function workerOutpatientSettlementCardParam() {
            var getCardParam = $.extend({}, params);
            if (passWordKey === "0") getCardParam["CardPwd"] = $("#CardPwd").val();
            $.ajax({
                type: 'POST',
                url: hostNew + '/WorkerOutpatientSettlementCardParam',
                data: getCardParam,
                dataType: "json",
                async: false,
                success: function (data) {
                    if (data.Success === false) {
                        var errData = data.Message;
                        layer.alert(errData, { skin: 'layui-layer-molv', icon: 5, title: '错误提示' });
                    } else {
                        var activeX = document.getElementById("CSharpActiveX");
                        //划卡支付
                      
                        var activeData = activeX.HospitalizationMethods(JSON.stringify(data.Data), JSON.stringify(baseInfo.HospitalInfo),"WorkerCardSettlement");
                        var activeJsonData = JSON.parse(activeData);
                        if (activeJsonData.Success === false) {
                            msgError(activeJsonData.Message);
                        } else {
                            workerOutpatientSettlementCard(activeJsonData.Data);
                        }

                     
                    }
                }

            });
        }

        //门诊划卡
        function workerOutpatientSettlementCard(settlementDataInfo) {
            var cardParam = $.extend({}, params);
            cardParam["SettlementJson"] = settlementDataInfo;
            $.ajax({
                type: 'POST',
                url: hostNew + '/WorkerOutpatientSettlementCard',
                data: cardParam,
                dataType: "json",
                async: false,
                success: function (data) {
                    if (data.Success === false) {
                        var errData = data.Message;
                        layer.alert(errData, { skin: 'layui-layer-molv', icon: 5, title: '错误提示' });
                    } else {
                        $('#CashPayment').val(data.Data.SelfPayFeeAmount);
                        layer.alert('划卡支付成功', { icon: 6, shade: 0.1, skin: 'layui-layer-molv' });
                        //添加预结算信息
                        if (data.Data.PayMsg !== null) {
                            settlementData(data.Data.PayMsg);
                        }
                    }
                }

            });
        }
        //职工划卡
        function workerSettlementCard() {
            if (passWordKey === "0") {
                var pwd = $("#CardPwd").val();
                if (pwd === "") {
                    msgError("医保卡密码不能为空!!!");
                    return false;
                } else
                {
                    if (pwd.length < 6) {
                        msgError("医保卡密码为6位,当前只有" + pwd.length + "位!!!");
                        return false;
                    } else
                    {
                        buttonStatus("SettlementCard", false);
                        workerOutpatientSettlementCardParam();
                    }
                }

            }
            else {
                buttonStatus("SettlementCard", false);
                workerOutpatientSettlementCardParam();
            }
        }
        $('#CardPwd').bind('keypress', function (event) {
          
            if (event.keyCode === 13) {
                workerSettlementCard();
            }
        });
      
